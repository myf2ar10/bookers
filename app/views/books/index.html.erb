  <body>
    <p id="notice"></p>

<h1>Books</h1>




<table>
  <thead>
    <tr>
      <th>
        Title
      </th>
      <th>
        Body
      </th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <tr>

<% @books.each do |book| %>
<!--<td>Title</td>まちがい-->
  <td><%= book.title %></td>
  <td><%= book.body %></td>
  <!--%= link_to "編集", edit_list_path(@list.id) %>見本-->
  <td><%= link_to "Show", book_path(book.id) %></td>
  <td><%= link_to "Edit", edit_book_path(book.id) %></td>
  <td><%= link_to "Destroy", book_path(book.id), method: :delete,"data-confirm" => "本当に削除しますか？" %></td>


    </tr>
<% end %>

  </tbody></table>
<h2>New book</h2>
  <% if @book.errors.any? %>
  <!--%= @books.errors.count %>  <!--件のエラーが発生しました-->-->
  <ul>
    <% @book.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
  </ul>
  <% end %>
<!--メンタ-->
<%= form_with model:@book, local:true do |book| %>

  <div class="field">
    <%= book.label :title %>
    <%= book.text_field :title %>
  </div>

  <div class="field">
    <%= book.label :body %>
    <%= book.text_area :body %>
  </div>

  <div class="field">
    <%= book.submit %>
  </div>

<% end %>

